{
  "manifest_version": 2,
  "name": "CircleCI Build Monitor",
  "version": "2.0",
  "description": "Chrome extension to display branch based build statuses",
  "permissions": [
    "storage",
    "*://*/*"
  ],

  "options_page": "build-monitor.html",

  "web_accessible_resources": ["images/*.png"],

  "browser_action": {
    "default_title": "CircleCI Monitor"
  }
}
